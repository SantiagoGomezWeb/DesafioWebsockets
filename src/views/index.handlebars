<h1>PRODUCTOS</h1>

<div class="row mt-4">
    {{#each valueReturned }}
    <div class="card text-white bg-success mb-3 mx-4 my-4" >
        <div class="card-header">code: {{this.code}}</div>
        <div class="card-body">
            <h3 class="card-title">{{this.title}}</h3>
            <p class="card-text">
                <li>
                    id: {{this.id}}
                </li>
                <li>
                    description: {{this.description}}
                </li>
                <li>
                    price: ${{this.price}}
                </li>
                <li>
                    category: {{this.category}}
                </li>
                <li>
                    status: {{this.status}}
                </li>
                <li>
                    stock: {{this.stock}}
                </li>
                <li>
                    thumbnail: {{this.thumbnail}}
                </li>
            </p>
        </div>
    </div>


    {{/each}}
</div>